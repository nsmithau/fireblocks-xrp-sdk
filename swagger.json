    "CredentialCreateRequest": {
      "type": "object",
      "required": ["subject", "credentialType"],
      "properties": {
        "subject": {
          "type": "string",
          "description": "The subject of the credential (XRPL address)",
          "example": "rPT1Sjq2YGrBMTttX4GZHjKu9dyfzbpAYe"
        },
        "credentialType": {
          "type": "string",
          "description": "Hex string, 1–64 bytes (2–128 hex chars). Type of credential entry.",
          "example": "AABBCCDDEEFF"
        },
        "expiration": {
          "type": "integer",
          "description": "Expiration time in seconds since Ripple Epoch (UInt32). Optional.",
          "example": 725846400
        },
        "uri": {
          "type": "string",
          "description": "Arbitrary data about the credential, as HEX string (1–256 bytes, 2–512 hex chars). Optional.",
          "example": "68747470733A2F2F6578616D706C652E636F6D2F637265645F31"
        },
        "flags": {
          "type": "integer",
          "description": "Optional transaction flags."
        },
        "memos": {
          "type": "array",
          "items": { "$ref": "#/definitions/Memo" },
          "description": "Optional memos array."
        }
      }
    },
