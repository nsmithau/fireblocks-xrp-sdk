          {
            "name": "vaultAccountId",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "Fireblocks vault account ID"
          }
        ],
        "responses": {
          "200": {
            "description": "OfferCreateResponse",
            "schema": {
              "$ref": "#/definitions/OfferCreateResponse"
            }
          }
        },
        "requestBody": {
          "required": true,
